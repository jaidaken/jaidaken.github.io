{
  "name": "vite-reactts-eslint-prettier",
  "version": "0.0.0",
  "homepage": "https://jaidaken.github.io/",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "cp 404.html dist/ && gh-pages -d dist",
		"dev": "concurrently --kill-others \"vite\" \"npm run server\"",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "server": "nodemon server.js",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "type-check": "tsc"
  },
  "dependencies": {
    "axios": "^1.1.2",
    "bootstrap": "5.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "framer-motion": "^7.0.0",
    "gsap": "^3.10.4",
    "nodemailer": "^6.8.0",
    "nodemon": "^2.0.20",
    "protonmail-api": "^2.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-just-parallax": "^3.1.14",
    "react-router-dom": "6",
    "typeit-react": "^2.6.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@vitejs/plugin-react": "^2.0.0",
    "concurrently": "^7.4.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "gh-pages": "^4.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1",
    "sass": "^1.54.3",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "license": "MIT"
}
